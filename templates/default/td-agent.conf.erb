<source>
  type syslog
  port 5140
  bind 0.0.0.0
  tag system.local
</source>

<source>
  type tail
  path /var/log/nginx/kibana.localhost.access.log #...or where you placed your Apache access log
  pos_file /var/log/td-agent/kibana.localhost.access.log.pos # This is where you record file position
  tag nginx.access #fluentd tag!
  format nginx # Do you have a custom format? You can write your own regex.
</source>

<match **>
  type elasticsearch
  logstash_format true
</match>


